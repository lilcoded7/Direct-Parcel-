{
  "version": 2,
  "builds": [
    {
      "rewrites":  [
        {"source": "/(.*)", "destination": "/"}
      ]
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "service/wsgi.py"
    }
  ]
}